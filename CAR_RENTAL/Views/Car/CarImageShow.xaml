<Window x:Class="CAR_RENTAL.Views.Car.CarImageShow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CAR_RENTAL.Views.Car"
        mc:Ignorable="d"
        Title="CarImageShow" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="56*"/>
            <RowDefinition Height="15*"/>
        </Grid.RowDefinitions>
        <Image x:Name="showCarImg" HorizontalAlignment="Left" Height="202" Margin="10,10,0,0" VerticalAlignment="Top" Width="277"/>
        <Button Content="Close" Grid.Row="1" Click="btnCloseShowImg" Margin="101,10,101,18"/>
    </Grid>
</Window>
